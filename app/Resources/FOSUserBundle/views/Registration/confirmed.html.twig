{% extends "dashbord.html.twig" %}

{% trans_default_domain 'FOSUserBundle' %}

{% block body %}
    <h3>{{ 'registration.confirmed'|trans({'%username%': user.username}) }}</h3>
    {% if targetUrl %}
    <p><a href="{{ targetUrl }}">{{ 'registration.back'|trans }}</a></p>
    {% endif %}
{% endblock body %}
